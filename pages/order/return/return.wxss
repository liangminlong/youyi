@import "../sent-item/sent-item-template.wxss";

page {
  background-color: #eee;
}

.list-container {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 120rpx;
}

.SearchBar {
  position: fixed;
  top: 0;
  z-index: 9999;
  width: 100%;
  height: 120rpx;
  background: white;
  display: flex;
  align-items: center;
  border-bottom: 1px solid #eee;
}

.search-cell{
  background-color: #eee;
  border-radius: 6rpx;
  width: 100%;
  height: 80rpx;
  margin:0 20rpx 0 20rpx;
  display: flex;
  align-items: center;
}

.icon-search{
  width: 28rpx;
  height: 28rpx;
  margin-left: 30rpx;
  margin-right: 10rpx;
}
.search-placeholder{
  font-size:30rpx;
  color: #666;

}